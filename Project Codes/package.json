{
  "name": "mini-world-explorer",
  "version": "1.0.0",
  "description": "An augmented reality-based mobile educational application for children to learn about geography, culture, and language",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace MiniWorldExplorer.xcworkspace -scheme MiniWorldExplorer -configuration Release"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.73.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.25.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-vector-icons": "^10.0.2",
    "react-native-linear-gradient": "^2.8.3",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "react-native-svg": "^13.14.0",
    "lottie-react-native": "^6.4.0",
    "react-native-permissions": "^3.10.1",
    "react-native-device-info": "^10.11.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-maps": "^1.7.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-modal": "^13.0.1",
    "react-native-animatable": "^1.3.3",
    "react-native-progress": "^5.0.0",
    "react-native-super-grid": "^4.1.0",
    "react-native-parallax-scroll-view": "^0.21.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "keywords": [
    "react-native",
    "augmented-reality",
    "education",
    "geography",
    "culture",
    "language-learning",
    "children",
    "mobile-app"
  ],
  "author": "Mini World Explorer Team",
  "license": "MIT"
} 